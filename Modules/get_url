# http://docs.ansible.com/ansible/latest/get_url_module.html

--- 
- hosts: all
  become: yes
  tasks: 
  - name: copy module
    copy: 
      src: /home/javed/
      dest: /home/javed/myenv
  - name: Get_url for downloading from http
    get_url: 
      url: http://ftp.yz.yamagata-u.ac.jp/pub/misc/jenkins/war-stable/2.73.2/jenkins.war
      dest: /etc/foo.conf
      mode: 0440
    
