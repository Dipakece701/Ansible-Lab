ubuntu@ansible-master:~/ansible/Lab-10-firewalldiabled$ cat inventory.txt 
[agent1]
172.31.31.81 ansible_connection=ssh nameserver_ip=100.100.10.10 snmp_port=160-161 http_port=8081 inter_ip_range=192.0.220.0/24

[agent2]
172.31.30.160 ansible_connection=ssh

[allservers:children]
agent1
agent2


ubuntu@ansible-master:~/ansible/Lab-10-firewalldiabled$ cat playbook.firewalldisabled.yml 
-
  name: Play1
  become: yes
  hosts: agent1
  tasks: 
    -
      name: Set firewall configurations
      firewalld:
        service: https
        permanent: true
        state: disabled

    -
      name: Disabled http_port configurations
      firewalld:
        port: '{{ http_port }}'/tcp
        permanent: true
        state: disabled

// Try the execution from your end


